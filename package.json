{
  "name": "react-css-modules-starter-kit",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "clean-output": "rimraf output",
    "validate": "npm-run-all --parallel validate-webpack:* lint",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "copy-files": "cpy input/index.html output",
    "clean-and-copy": "npm run clean-output && npm run copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --env.dev --content-base output",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "lint": "eslint .",
    "setup": "npm install && npm run validate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "classnames": "^2.2.5",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-react": "^5.2.2",
    "immutable": "^3.8.1",
    "npm-run-all": "^2.3.0",
    "postcss-autoreset": "^1.1.5",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "postcss-modules": "^0.5.0",
    "postcss-modules-values": "^1.1.3",
    "postcss-normalize": "^0.3.0",
    "ramda": "^0.21.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.18",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-validator": "^2.2.3"
  }
}
